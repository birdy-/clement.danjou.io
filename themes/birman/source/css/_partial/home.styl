.home-inner
    color: #000
    background: #fff
    box-sizing: border-box
    .home-intro
        display: flex
        align-items: center
        margin: 0 auto
        font-size: 2.2em
        background: linear-gradient(#33363b, #33363b), linear-gradient(#666, #666), linear-gradient(#999, #999), linear-gradient(#bbb, #bbb)
        background-color: #eee
        background-size: 3vw 3vw, 6vw 6vw, 9vw 9vw, 12vw 12vw
        background-repeat: no-repeat
        padding: 20px 40px 20px 16vw
        box-sizing: border-box;
        min-height: 12vw
    .home-content
        padding: 40px
        ul
            display: flex
            flex-wrap: wrap
            align-items: flex-end
            justify-content: space-around
            li
                margin: 10px
                list-style: none
    .home-contact-me
      text-align: center
